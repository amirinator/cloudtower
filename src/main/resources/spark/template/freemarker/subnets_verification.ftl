<html><#include "./navigation.ftl"><body>    <h2>AWS Subnets Audit</h2>    <table class="tablesorter">      <thead>        <tr>            <th>Subnet Name</th>            <th>Subnet ID</th>            <th>VPC Name</th>            <th>Availability Zone</th>            <th>Account</th>        </tr>        </thead>        <tbody>        <#list subnets as subnet,accountname>          <tr>            <#if subnet.subnetName??>                <td>${subnet.subnetName}</td>            </#if>            <#if subnet.awsSubnetId??>                <td>${subnet.awsSubnetId}</td>            </#if>            <#if subnet.vpcName??>                <td>${subnet.vpcName}</td>            </#if>            <#if subnet.availabilityZone??>                <td>${subnet.availabilityZone}</td>            </#if>            <#if accountname??>                <td>${accountname}</td>            </#if>          </tr>        </#list>        </tbody>    </table>  </body></html>