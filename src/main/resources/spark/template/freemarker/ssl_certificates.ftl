<html><#include "./navigation.ftl"><body><h1>AWS SSL Certificate Inventory</h1>    <table class="tablesorter">        <thead>        <tr>            <th>ELB</th>            <th>Expiration Date</th>            <th>Account</th>        </tr>        </thead>        <tbody>    <#list ssl_certificates as ssl_certificate,accountname>        <tr>            <#if ssl_certificate.hostname??>                <td>${ssl_certificate.hostname}</td>            <#else>                <td>N/A</td>            </#if>            <#if ssl_certificate.expirationDate??>                <#if ssl_certificate.expirationDate?date <= yesterday?date>                    <td class="expiration_date_expired">${ssl_certificate.expirationDate?iso_utc}</td>                <#elseif ssl_certificate.expirationDate?date < oneMonthFromToday?date && yesterday?date < ssl_certificate.expirationDate?date>                    <td class="expiration_date_warning">${ssl_certificate.expirationDate?iso_utc}</td>                <#else>                    <td>${ssl_certificate.expirationDate?iso_utc}</td>                </#if>            <#else>                <td>N/A</td>            </#if>            <#if accountname??>                <td>${accountname}</td>            <#else>                <td>N/A</td>            </#if>        </tr>    </#list>        </tbody>    </table></body></html>