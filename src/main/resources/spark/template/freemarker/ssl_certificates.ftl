<html><#include "./navigation.ftl"><body><script type="application/javascript">    $(document).ready(function() {        // call the tablesorter plugin        $("table").tablesorter({            // allow for sorting on columns, order asc            sortList: [[1,1],[2,1],[3,1]]        });    });</script><h2>AWS SSL Certificate Inventory</h2>    <table class="tablesorter">        <thead>        <tr>            <th>ELB</th>            <th>Type</th>            <th>Expiration Date</th>            <th>Account</th>        </tr>        </thead>        <tbody>    <#list ssl_certificates as ssl_certificate,accountname>        <tr>            <#if ssl_certificate.hostname??>                <td>${ssl_certificate.hostname}</td>            <#else>                <td>Unknown</td>            </#if>            <#if ssl_certificate.certificateType??>                <td>${ssl_certificate.certificateType}</td>            <#else>                <td>Unknown</td>            </#if>            <#if ssl_certificate.expirationDate??>                <#if ssl_certificate.expirationDate?date <= yesterday?date>                    <td class="expiration_date_expired">${ssl_certificate.expirationDate?iso_utc}</td>                <#elseif ssl_certificate.expirationDate?date < oneMonthFromToday?date && yesterday?date < ssl_certificate.expirationDate?date>                    <td class="expiration_date_warning">${ssl_certificate.expirationDate?iso_utc}</td>                <#else>                    <td>${ssl_certificate.expirationDate?iso_utc}</td>                </#if>            <#else>                <#if ssl_certificate.certificateType == 'ACM'>                    <td>N/A</td>                <#else>                    <td>Unknown</td>                </#if>            </#if>            <#if accountname??>                <td>${accountname}</td>            <#else>                <td>Unknown</td>            </#if>        </tr>    </#list>        </tbody>    </table></body></html>