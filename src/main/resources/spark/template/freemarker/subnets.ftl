<html><#include "./navigation.ftl"><body><script type="application/javascript">    function chgAction(selectedObject) {        var form = document.subnet_form;        form.vpc.value = selectedObject.options[selectedObject.options.selectedIndex].value;    }</script><h2>AWS Subnets Audit</h2><table>    <form id="subnet_form" name="subnet_form" method="post" action="/subnets/vpcs" enctype="application/x-www-form-urlencoded">        <div class="form-group">            <label class"form-label"> VPC Name </label>            <select class="form-label" name="vpc_name" onchange="chgAction(this)">            <option value="All" selected="selected">All</option>            <#list vpcs as vpc>                <option value='${(vpc!" ")}'>${vpc}</option>            </#list>            </select>            <input type="hidden" value="All" name="vpc"/>            <input type="Submit" value="Submit" />        </div>    </form></table><table class="tablesorter">    <thead>    <tr>        <th>Account</th>        <th>Region</th>        <th>VPC Name</th>        <th>VPC ID</th>        <th>Subnet Name</th>        <th>Subnet ID</th>        <th>CIDR</th>        <th>Available IP Count</th>    </tr>    </thead>    <tbody>    <#list subnets as subnet,accountname>    <tr>        <#if accountname??>            <td>${accountname}</td>        </#if>        <#if subnet.regionName??>            <td>${subnet.regionName}</td>        </#if>        <#if subnet.vpcName??>            <td>${subnet.vpcName}</td>        </#if>        <#if subnet.vpcId??>            <td>${subnet.vpcId}</td>        </#if>        <#if subnet.subnetName??>            <td>${subnet.subnetName}</td>        </#if>        <#if subnet.awsSubnetId??>            <td>${subnet.awsSubnetId}</td>        </#if>        <#if subnet.cidrBlock??>            <td>${subnet.cidrBlock}</td>        </#if>        <#if subnet.ipCount??>            <td>${subnet.ipCount}</td>        </#if>    </tr>    </#list>    </tbody></table></body></html>