<html><#include "./navigation.ftl"><body><div class="layout"><script type="application/javascript">    function chgAction(selectedObject) {        var form = document.subnet_form;        form.vpc.value = selectedObject.options[selectedObject.options.selectedIndex].value;    }    $(document).ready(function() {        // call the tablesorter plugin        $("table").tablesorter({            // allow for sorting on columns, order asc            sortList: [[1,1],[2,1],[3,1],[4,1],[5,1],[6,1],[7,1],[8,1]]        });    });</script><h2>AWS Subnets Inventory</h2><table>    <form id="subnet_form" name="subnet_form" method="post" action="/subnets" enctype="application/x-www-form-urlencoded">        <div class="form-group">            <label class"form-label"> VPC Name </label>            <select class="form-label" name="vpc_name" onchange="chgAction(this)">            <option value="All">All</option>            <#list vpcs as vpc>                <option value='${(vpc!" ")}'  <#if vpc == vpc_selected> selected = "selected"</#if>    >${vpc}</option>            </#list>            </select>            <input type="hidden" value="All" name="vpc"/>            <input type="Submit" value="Submit" />        </div>    </form></table><table class="tablesorter">    <thead>    <tr>        <th>Account</th>        <th>Region</th>        <th>VPC Name</th>        <th>VPC ID</th>        <th>Subnet Name</th>        <th>Subnet ID</th>        <th>CIDR</th>        <th>Available IP Count</th>    </tr>    </thead>    <tbody>    <#list subnets as subnet,accountname>    <tr>        <#if accountname??>            <td>${accountname}</td>        <#else>            <td>N/A</td>        </#if>        <#if subnet.regionName??>            <td>${subnet.regionName}</td>        <#else>            <td>N/A</td>        </#if>        <#if subnet.vpcName??>            <td>${subnet.vpcName}</td>        <#else>            <td>N/A</td>        </#if>        <#if subnet.vpcId??>            <td>${subnet.vpcId}</td>        <#else>            <td>N/A</td>        </#if>        <#if subnet.subnetName??>            <td>${subnet.subnetName}</td>        <#else>            <td>N/A</td>        </#if>        <#if subnet.awsSubnetId??>            <td>${subnet.awsSubnetId}</td>        <#else>            <td>N/A</td>        </#if>        <#if subnet.cidrBlock??>            <td>${subnet.cidrBlock}</td>        <#else>            <td>N/A</td>        </#if>        <#if subnet.ipCount??>            <td>${subnet.ipCount}</td>        <#else>            <td>N/A</td>        </#if>    </tr>    </#list>    </tbody></table></div></body></html>