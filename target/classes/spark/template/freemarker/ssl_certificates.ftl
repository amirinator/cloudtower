<html><head></head><body><h2>AWS SSL Certificate Inventory</h2><#list ssl_certificates as ssl_certificate><div class="ssl_certificate">    <table>        <thead>        <tr>            <th>ELB</th>            <th>Expiration Date</th>            <th>Account</th>        </tr>        </thead>        <tbody>        <tr>            <#if ssl_certificate.hostname??>                <td>${ssl_certificate.hostname}</td>            </#if>            <#if ssl_certificate.expirationDate??>                <td>${ssl_certificate.expirationDate}</td>            </#if>            <#if ssl_certificate.accountName??>                <td>${ssl_certificate.accountName}</td>            </#if>        </tr>        </tbody>    </table></div></#list></body></html>